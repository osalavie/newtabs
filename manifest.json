{
    "manifest_version": 3,
    "name": "New Tab",
    "version": "1.0",
    "description": "Customize the new tab page",
    "permissions": ["tabs", "storage"],
    "action": {
      "default_popup": "html/popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "chrome_url_overrides": {
      "newtab": "html/newtab.html"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  